<section class="py-16">
  <div class="container mx-auto px-4">
    <!-- En-tte avec titre et bouton menu -->
    <div class="flex items-center justify-between mb-10">
      <h2 class="text-[14px] font-[700] text-card-foreground">
        Liste des prochains vnements
      </h2>

      <!-- Bouton hamburger menu -->
      <button
        class="flex items-center gap-2 text-gray-700 hover:text-primary transition-colors"
        aria-label="Menu des filtres"
      >
        <svg
          width="20"
          height="20"
          viewBox="0 0 20 20"
          fill="none"
          xmlns="http://www.w3.org/2000/svg"
        >
          <path
            d="M2 4H18M2 10H18M2 16H18"
            stroke="currentColor"
            stroke-width="2"
            stroke-linecap="round"
            stroke-linejoin="round"></path>
        </svg>
      </button>
    </div>

    <!-- Grille d'vnements -->
    <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-6">
      {
        events.map((event) => (
          <article class="bg-white rounded-2xl overflow-hidden  transition-shadow group">
            {/* Image */}
            <div class="relative h-48 rounded-2xl overflow-hidden">
              <img
                src={event.image}
                alt={event.title}
                class="w-full h-full object-cover  transition-transform duration-300"
              />
            </div>

            {/* Contenu */}
            <div class="relative">
              {event.category && (
                <BadgeCorner
                  orientation="horizontal"
                  position="left"
                  class="rounded-2xl"
                  style="border-bottom-left-radius: 10px;"
                >
                  {event.category}
                </BadgeCorner>
              )}
              <div class="relative z-10 py-5 bg-white">
                {/* Titre */}
                <h3 class="text-[22px] font-bold text-gray-900 mb-2 line-clamp-2 leading-none">
                  {event.title}
                </h3>

                {/* Localisation */}
                <p class="text-sm text-gray-600 mb-3">{event.location}</p>

                {/* Description */}
                <p class="text-sm text-gray-600 mb-4 line-clamp-3 leading-none">
                  {event.description}
                </p>

                {/* Bouton Lire plus */}
                <a href={event.link}>
                  <Button class="p-1">
                    Inscription
                    {/* Lire plus
                  <svg
                    class="w-4 h-4"
                    fill="none"
                    stroke="currentColor"
                    viewBox="0 0 24 24"
                  >
                    <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      stroke-width="2"
                      d="M9 5l7 7-7 7"
                    />
                  </svg> */}
                  </Button>
                </a>
              </div>
            </div>
          </article>
        ))
      }
    </div>
  </div>
</section>

<style>
  .line-clamp-2 {
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
  }

  .line-clamp-3 {
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
    overflow: hidden;
  }
</style>
